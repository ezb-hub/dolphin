{
  "name": "javelin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "clean": "rm -rf .next",
    "dev": "yarn clean && cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "yarn clean && next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.20",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  }
}
